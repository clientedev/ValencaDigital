    "vercel-build": "npm run build:client"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.12.0",
    "@tanstack/react-query": "^5.59.14",
    "connect-pg-simple": "^10.0.0",
    "drizzle-orm": "^0.36.1",
    "drizzle-zod": "^0.5.1",
    "express": "^4.21.1",
    "express-session": "^1.18.1",
    "memorystore": "^1.6.7",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "wouter": "^3.3.5",
    "zod": "^3.23.8"
  }
}